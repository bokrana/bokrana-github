/*
=========== admin control  ============================
*/
#admin-control{
    background: none repeat scroll 0 0 #BE5C66;
    border-bottom: 2px solid #A2404A;
    color: #FFFFFF;
    font-size: 14px;
    font-weight: normal;
    height: 40px;
    line-height: 40px;
    margin: 0;
    overflow: visible;
    padding: 0;
    position: relative;
    text-align: center;
    width: 100%;
    z-index: 1000;
    display: block;
    left: 0;
    top: 0;
     span{float: left;font-family: RobotoMedium;margin-left: 13%;}
     nav{float: right;margin-right: 20%;}
     li{float: left;position: relative;}
     
     li:hover{ background-color: #A2404A;}
     
     li:after { background: none repeat scroll 0 0 #A2404A;
    content: "";
    height: 20px;
    position: absolute;
    top: 8px;
    width: 2px;}
    
     li a {color: rgb(255, 255, 255);
          font-size: 14px;
          padding: 10px 15px;
          text-transform: uppercase;}
}

/*=============end of admin control==========================*/

.loading-mask {background-color: rgba(255, 255, 255, 0.8);
    border-radius: 3px;bottom: 0;left: 0;position: absolute;right: 0;top: 0;}
    
/*=============end of header================================*/
span.icon-heart-empty:before, span.icon-edit-1:before,span.icon-logout-1:before{margin-right: 0 !important;}
.input-error{border-color: #F27474 !important;}   
.login-controller-error,.restor-controller-error,.new-passionate-controller-error,.restore-password-controller-error,.new-passion-controller-error{ background-image: image-url("error-icon.png");
display:none;margin: 10px 0;
background-color: #E8534F;
background-position: 15px center;
    background-repeat: no-repeat;
    border-radius: 2px;
    color: #FFFFFF;
   box-sizing: border-box;
    overflow: hidden;
    padding: 15px 15px 15px 50px;
    position: relative;
    width: 100%;}

.login-controller-error,restor-controller-error,new-passionate-controller-error ,.restore-password-controller-error ,.new-passion-controller-error > p{line-height: 20px;word-wrap: break-word;font-size: 1.2em;}
.controller-error-link{ color: white;text-decoration: underline; font-family: RobotoBold;cursor: pointer;}
.controller-error-link:hover{text-decoration:none}
