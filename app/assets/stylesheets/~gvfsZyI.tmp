input[type='email']:focus , input[type="password"]:focus {border-color: #51a9ab;}

input[type=submit] {border: medium none;box-shadow: none;color: #FFFFFF; cursor: pointer;font-size: 1.2em; font-family: Montserrat-Regular,"Helvetica Neue",Arial;
